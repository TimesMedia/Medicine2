<Window x:Class="Medicine2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medicine2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <TabControl>
        <TabItem Header="Imports">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal">
                    <Button x:Name="buttonImportWord" Click="buttonImportWord_Click" Height="30" Margin="10,10,10,10">Import Word Documents</Button>
                    <Button x:Name="buttonTryReadWord" Click="buttonTryReadWord_Click" Height="30" Margin="10,10,10,10">Try to read Word Documents</Button>
                </StackPanel>
            </Grid>
        </TabItem>
       
    </TabControl>
</Window>
